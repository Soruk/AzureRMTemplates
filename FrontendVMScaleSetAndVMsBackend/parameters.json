{
  "$schema":
    "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "environmentName": {
      "value": "myPlatform"
    },
    "environmentNumber": {
      "value": 1
    },
    "environmentType": {
      "value": "Dev"
    },
    "availibiltyZone": {
      "value": 1
    },
    "vmSizeWeb": {
      "value": "Standard_B2s"
    },
    "vmSizeDb": {
      "value": "Standard_B2s"
    },    
    "vmSizeApp": {
      "value": "Standard_B2s"
    },
    "vmAdminUsername": {
      "value": "vmAdminName"
    },
    "vmAdminPassword": {
      "value": "vmAdminPwd"
    },
    "instancesCount": {
      "value": 1
    },  
    "authorizedImportP2PIpAddresses": {
      "value": [["127.0.0.1", "1.1.1.1"]]
    },
    "customWebDomains":{
      "value":  ["name1.mydomain.com", "name2.mydomain.com"]
    },
    "resourcesStorageAccountName": {
      "value": "storageAccountName"
    },
    "resourcesStorageAccountKey": {
      "value": "storageAccountKey"
    }
  }
}
